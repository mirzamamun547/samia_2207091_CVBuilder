<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo1.scene1Controller"
            prefHeight="500.0" prefWidth="600.0">
    <children>
        <Label layoutX="166" layoutY="60" text="Name"/>
        <TextField fx:id="namebutton" layoutX="248" layoutY="56"/>

        <Label layoutX="166" layoutY="100" text="Email"/>
        <TextField fx:id="emailbutton" layoutX="248" layoutY="96"/>

        <Label layoutX="166" layoutY="140" text="Date of Birth"/>
        <TextField fx:id="dobbutton" layoutX="248" layoutY="136"/>

        <Label layoutX="166" layoutY="180" text="Nationality"/>
        <TextField fx:id="nationalitybutton" layoutX="248" layoutY="176"/>

        <Label layoutX="166" layoutY="220" text="Education"/>
        <TextField fx:id="educationbutton" layoutX="248" layoutY="216" prefWidth="250"/>

        <Label layoutX="166" layoutY="260" text="Skills"/>
        <TextField fx:id="skillsbutton" layoutX="248" layoutY="256" prefWidth="250"/>

        <Label layoutX="166" layoutY="300" text="Experience"/>
        <TextField fx:id="experiencebutton" layoutX="248" layoutY="296" prefWidth="250"/>

        <Button layoutX="264" layoutY="360" text="Login" onAction="#login"/>
    </children>
</AnchorPane>
